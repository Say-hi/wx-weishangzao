{
//  微商造小程序页面分布
  "pages": [
    "pages/posterOne/posterOne", // 1号海报模板
    "pages/map/map", // 微商地图
    "pages/index/index", // 主页
    "pages/posterCollect/posterCollect", // 海报收藏夹
    "pages/posterShow/posterShow", // 海报展示
    "pages/supportPage/supportPage",  // 微商人物支持页面
    "pages/supportTeamPage/supportTeamPage", // 微商团队支持页面
    "pages/applyTeam/applyTeam", // 团队申请
    "pages/upload/upload", // 上传图片
    "pages/weishang/weishang", // 我是微商
    "pages/customization/customization", // 个性化定制
    "pages/ktPoster/ktPoster", // 海报开通
    "pages/homepage/homepage", // 我的微商主页
    "pages/followWs/followWs", // 我关注的微商
    "pages/RHO/RHO", // 我关联的上家
    "pages/relevanceTeam/relevanceTeam", // 我关联的团队
    "pages/relevanceAgent/relevanceAgent", // 关联我的代理
    "pages/user/user", // 个人中心
    "pages/ruler/ruler", // 规则
    "pages/ranking/ranking" // 排行榜
    // Dont remove this comment
  ],
  // Window configuration
  // https://mp.weixin.qq.com/debug/wxadoc/dev/framework/config.html#window
  "window": {
    // 类型：HexColor；默认值：#000000
    // 导航栏背景颜色，如"#000000"
    "navigationBarBackgroundColor": "#fedb43",
    // 类型：String；默认值：white
    // 导航栏标题颜色，仅支持 black/white
    "navigationBarTextStyle": "black",
    // 类型：String；默认值：无
    // 导航栏标题文字内容
    "navigationBarTitleText": "微商造",
    // 类型：HexColor；默认值：#ffffff
    // 窗口的背景色
    "backgroundColor": "#fff",
    // 类型：String；默认值：dark
    // 下拉背景字体、loading 图的样式，仅支持 dark/light
    "backgroundTextStyle": "light",
    // 类型：Boolean；默认值：false
    // 是否开启下拉刷新，详见页面相关事件处理函数。
    // https://mp.weixin.qq.com/debug/wxadoc/dev/framework/app-service/page.html?t=1476197491005#页面相关事件处理函数
    "enablePullDownRefresh": false
  },
  // Tab bar configuration
  // https://mp.weixin.qq.com/debug/wxadoc/dev/framework/config.html#tabBar
  "tabBar": {
    "color": "#999999",
    "selectedColor": "#fedb43",
    "borderStyle": "black",
    "backgroundColor": "#fff",
    "list": [
      {
        "text": "首页",
        "pagePath": "pages/index/index",
        "iconPath": "images/home.png",
        "selectedIconPath": "images/home_hl.png"
      },
      {
        "text": "排行榜",
        "pagePath": "pages/ranking/ranking",
        "iconPath": "images/rank.png",
        "selectedIconPath": "images/rank_hl.png"
      },
      {
        "text": "我的",
        "pagePath": "pages/user/user",
        "iconPath": "images/user.png",
        "selectedIconPath": "images/user_hl.png"
      }
    ]
  },
  // Network timeout configuration
  // https://mp.weixin.qq.com/debug/wxadoc/dev/framework/config.html#networkTimeout
  "networkTimeout": {
    "request": 10000,
    "downloadFile": 10000
  },
  // Debug log to console
  // https://mp.weixin.qq.com/debug/wxadoc/dev/framework/config.html#debug
  "debug": true
}
