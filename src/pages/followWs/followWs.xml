<!--我关注的微商-->
<view>
  <view class="agent" wx:for="{{ agentArr }}">
    <view class="a-top">
      <image class="br" src="{{ item.img }}"></image>
      <text>{{ item.name }}</text>
    </view>
    <text>{{ item.sign }}</text>
    <view data-index="{{ index }}" catchtap="del" class="del">取消关注</view>
  </view>
</view>